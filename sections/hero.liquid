     align-items: flex-start;
   }

   .hero-align-center {
     text-align: center;
     align-items: center;
   }

   .hero-align-right {
     text-align: right;
     align-items: flex-end;
   }


   .hero-container-align-left {
     left: 0;
     transform: translate(0, -50%);
   }

