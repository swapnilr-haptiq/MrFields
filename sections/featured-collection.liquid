<section id="section-{{ section.id }}" class="featured-collection">
  <div class="container">
    <h2 class="section-title">{{ section.settings.heading }}</h2>
    <p class="filter-text">{{ section.settings.filter_text }}</p>
    {% render 'gift_filters',
      products: collections[section.settings.collection].products,
      show_treat_filter: section.settings.show_treat_filter,
      show_occasion_filter: section.settings.show_occasion_filter,
      show_budget_filter: section.settings.show_budget_filter
    %}

    <div class="products-card-section">
      {% assign selected_collection = section.settings.collection %}
      {% if selected_collection != blank %}
        {% assign product_collection = collections[selected_collection] %}
        <p class="total-items">{{ product_collection.products.size }} items</p>

        <div class="product-container">
          <div class="featured-product-list slick-slider">
            {% for product in product_collection.products %}
              <div class="product-item">
                {% render 'product-detail', product: product %}
              </div>
            {% endfor %}
          </div>
        </div>
      {% endif %}
    </div>
    <div class="button-wrapper">
      <button class="leftBtn">
        <img src="{{ 'leftA.png' | asset_url }}" alt="Left Arrow">
      </button>
      <button class="rightBtn">
        <img src="{{ 'RightA.png' | asset_url }}" alt="Right Arrow">
      </button>
    </div>
  </div>
</section>





<style>
  #section-{{ section.id }} {
    --background-color: {{ section.settings.background_color }};
    --text-color: {{ section.settings.text_color }};
    --card-background-color: {{ section.settings.card_background_color }};
    --card-text-color: {{ section.settings.card_text_color }};
    --slider-button-background-color: {{ section.settings.slider_button_background_color }};
    --filter-background-color: {{ section.settings.filter_background_color }};
    --filter-text-color: {{ section.settings.filter_text_color }};
    --discount-background-color: {{ section.settings.discount_background_color }};
    --discount-text-color: {{ section.settings.discount_text_color }};
    --slick-dots-bg: {{ section.settings.slick_dots_bg }}
  }
</style>

