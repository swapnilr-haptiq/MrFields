@import url("https://fonts.googleapis.com/css2?family=Patua+One&display=swap");
@import url("https://fonts.googleapis.com/css2?family=Assistant:wght@200..800&display=swap");
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
}

/* HTML5 display-role reset for older browsers */
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  display: block;
}

body {
  line-height: 1;
}

ol,
ul {
  list-style: none;
}

blockquote,
q {
  quotes: none;
}

blockquote:before,
blockquote:after,
q:before,
q:after {
  content: "";
  content: none;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

// genral styling
button {
  margin: 0;
  padding: 0;
  background-color: transparent;
  border: none;
  appearance: none;
  box-shadow: none;
  font: inherit;
  outline: none;
}

a {
  text-decoration: none;
  color: black;
}

::selection {
  background-color: black;
  color: white;
}

@mixin phone {
  @media (max-width: 599px) {
    @content;
  }
}

@mixin tablet {
  @media (max-width: 900px) {
    @content;
  }
}

@mixin desktop {
  @media (max-width: 1200px) {
    @content;
  }
}

// Start Preset Section

// .header
.container,
.footer {
  padding: 0 25px;
}

body {
  font-weight: 700;
  font-size: 16px;
  line-height: 1.25;
  font-family: "Arial", Helvetica, sans-serif;
  -webkit-font-smoothing: antialiased;
}

main {
  // padding-top: 72px;

  @include phone {
    padding-top: 0;
  }
}

.flex {
  display: flex;
}

.space-between {
  justify-content: space-between;
}

.column {
  flex-direction: column;
}

.half {
  width: 50%;
}

*,
::after,
::before {
  box-sizing: border-box;
}

button,
input,
option,
textarea {
  margin: 0;
  padding: 0;
  background-color: transparent;
  border: none;
  appearance: none;
  box-shadow: none;
  outline: none;
}

.section {
  padding: 25px;
}

img,
picture {
  width: 100%;
  vertical-align: middle;
}

// start header
.header {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: white;
  z-index: 10;

  @include phone {
    position: static;
  }
}

.header-inner {
  height: 72px;
  align-items: center;
  border-bottom: 1px solid;

  @include phone {
    display: none;
  }
}

.header-title {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
}

// Start Mobile Nav

.header-mobile {
  height: 72px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid black;
  display: none;

  @include phone {
    display: flex;
  }
}

.mobile-nav {
  position: fixed;
  bottom: 25px;
  left: 0;
  right: 0;
  padding: 0 25px;
  z-index: 10;
  display: flex;
  justify-content: space-between;
}

.mobile-nav-button {
  border: 1px solid black;
  padding: 10px;
  border-radius: 25px;
  background: white;
  z-index: 10;
}

// Start Collection

.collection-list {
  flex-wrap: wrap;
}

.collection-title {
  padding: 60px 0;
  font-size: 24px;
  border-bottom: 1px solid black;
  text-align: center;
  height: 1.2;
  // background: ;  // You may want to define a background color here if needed
}

// Start Product Card

.product-card {
  width: calc((100% - 25px * 2) / 3);
  margin-bottom: 32px;
  height: 100%;

  @include phone {
    width: 100%;
  }
}

.product-image-wrapper {
  // padding-top: 120%;
  display: block;
  overflow: hidden;
  position: relative;
  margin-bottom: 20px;
}

.swiper-container {
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.swiper-slide {
  display: flex;
  justify-content: start;
  align-items: center;
  position: relative;
  user-select: none;

  img {
    width: 100%;
    height: 100%;
    outline-style: none;
    box-shadow: none;
    border-color: transparent;
    object-fit: cover;
  }
}

.swiper-button-prev,
.swiper-button-next {
  position: absolute;
  top: 0;
  width: 25%;
  height: 100%;
  margin-top: 0;
  outline-style: none;
  box-shadow: none;
  border-color: transparent;

  &::after {
    content: none;
  }

  @include phone {
    display: none;
  }
}

.product-card-title {
  position: relative;
  margin: 16px 0;

  h3 {
    padding-right: 20px;
  }

  .add-button {
    width: 20px;
    height: 20px;
    position: absolute;
    top: 0;
    right: 0;
    border-radius: 50%;

    &::before {
      content: "";
      width: 100%;
      height: 100%;
      position: absolute;
      top: 0;
      left: 0;
      border: 1px solid black;
      border-radius: inherit;
    }

    .plus {
      &::after,
      &::before {
        content: "";
        width: 8px;
        height: 2px;
        position: absolute;
        top: 9px;
        left: 6px;
        background: currentColor;
      }

      &::before {
        transform: rotate(90deg);
      }
    }
  }
}

.product-card-title:hover .add-button {
  color: white;
  background-color: black;
}

.product-card-sizes {
  display: none;
}

.product-card-title:hover .product-card-price {
  display: none;
}

.product-card-title:hover .product-card-sizes {
  display: block;
}

// Start product template

.swiper-container-product-page {
  overflow: hidden;
  width: 100%;
  height: 100%;
}

.product-template {
  padding-top: 25px;
  display: grid;
  grid-template-columns: calc((100% - 25px) / 2) calc((100% - 25px) / 2);
  justify-content: space-between;
  margin-bottom: 32px;

  @include phone {
    grid-template-columns: 100%;
  }
}

.product-image:hover .swiper-button-prev::before,
.product-image:hover .swiper-button-next::before {
  opacity: 0;
}

.product-description-container {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.product-description {
  width: 50%;
  margin-bottom: 30px;

  @include phone {
    width: 100%;
  }
}

.product-title {
  margin-bottom: 100px;
  font-size: 24px;
  text-transform: uppercase;
}

.product-option-row {
  position: relative;
  user-select: none;
}

.product-option-label::before {
  // content: "";
  width: 11px;
  height: 11px;
  position: absolute;
  right: 25px;
  top: 50%;
  transform: translateY(-50%);
  background: currentColor;
  border-radius: 50%;
}

.product-option-label,
.add-to-cart-button {
  padding: 25px;
  position: relative;
  border: solid;
  cursor: pointer;
  border-color: black;
  border-width: 1px 1px 0;
  border-right-color: transparent;
  border-left-color: transparent;
  background-color: white;
  font-size: 16px;
  width: 100%;
  text-align: left;
}

.add-to-cart-button {
  text-transform: uppercase;

  &:hover {
    background-color: black;
    color: white;
  }
}

.product-option-label:hover {
  background: white;
}

.product-option-list {
  position: absolute;
  bottom: 70px;
  right: 0;
  left: 0;
  background-color: white;
  border: solid 1px black;
}

.product-option-item {
  padding: 25px;
  cursor: pointer;

  &:nth-child(n + 2) {
    box-shadow: inset 0 1px #9a9a9a;
  }

  &:hover {
    background: #9a9a9a;
  }
}

// Start mini cart

.mini-cart.hidden {
  transform: translateX(100%);

  .mini-cart-close-button {
    opacity: 0;
    transition: none;
  }
}

.mini-cart-close-button {
  cursor: pointer;
  position: absolute;
  top: 25px;
  left: 25px;
  right: auto;
  color: inherit;
  transition: opacity 0.5s 0.1s, padding 0.15s;
}

.mini-cart.hidden::before {
  opacity: 0;
}

.mini-cart {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  height: 100vh;
  width: 100%;
  z-index: 99;
  max-width: 350px;
  background-color: white;
  padding: 72px 25px 25px 25px;
  transition-property: transform;

  &::before {
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    -webkit-box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    transition-property: opacity;
    z-index: -1;
  }

  @include phone {
    position: fixed;
    width: calc(100vh - 50px);
    height: 100%;
    // overflow-x:
  }
}

.mini-cart,
.mini-cart::before {
  transition-duration: 1s;
  transition-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
}

.cart-form {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.cart-button {
  cursor: pointer;
  position: relative;
  transition: 0.15s;
  z-index: 999;

  &:hover {
    padding-right: 15px;
  }
}

.cart-button:hover .cart-arrow-icon {
  opacity: 1;
  right: 0;
}

.cart-arrow-icon {
  opacity: 0;
  width: 11.5px;
  height: 12px;
  position: absolute;
  top: 5px;
  right: -15px;
  transition: 0.15s;
  pointer-events: none;
}

.mini-cart-image-wrapper {
  width: 140px;
  margin-right: 25px;

  img {
    width: 100%;
  }
}

.mini-cart-rows {
  border-top: 1px solid black;
  border-bottom: 1px solid black;
  height: 100%;
  overflow: auto;
}

.mini-cart-item {
  display: flex;
  padding: 25px 0;
}

.mini-cart-item-right {
  flex: 1;
}

.mini-cart-quantity {
  height: 25px;
  overflow: hidden;
  margin-top: 25px;
  position: relative;
  border: 1px solid;
  border-radius: 13px;

  input {
    width: 100%;
    height: 100%;
    text-align: center;
    line-height: 25px;
  }

  .mini-cart-quantity-button {
    width: 25px;
    position: absolute;
    top: -4px;
    bottom: 0;
    left: 0;
    cursor: pointer;
  }

  .mini-cart-quantity-button:last-of-type {
    left: auto;
    right: 0;
  }
}

.checkout-button {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 50px;
  border: 25px;
  padding: 0 25px;
  position: relative;
  transition: 0.15s;
  padding-bottom: 1px;
  background: black;
  color: white;

  &:hover {
    opacity: 0.05s;
  }
}

.checkout-subtotal {
  display: flex;
  justify-content: space-between;
  height: 72px;
  padding: 0 25px;
  line-height: 72px;
}

.outside {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
}

.cart-empty {
  opacity: 0.2;
  padding-top: 42px;
  font-size: 24px;
  line-height: 1.5;
}

// start menu drawer
.menu-drawer {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  height: 100vh;
  width: 100%;
  z-index: 99;
  max-width: 350px;
  background-color: white;
  padding: 72px 25px 25px 25px;
  transition: transform 0.9s cubic-bezier(0.165, 0.84, 0.44, 1), left 0.5s;

  &::before {
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    box-shadow: 1px 3px 47px 37px rgba(0, 0, 0, 0.25);
    -webkit-box-shadow: 1px 3px 47px 37px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: 1px 3px 47px 37px rgba(0, 0, 0, 0.25);
    transition-property: opacity;
    z-index: -1;
  }

  @include phone {
    position: fixed;
    width: calc(100vw - 50px);
  }
}

.menu-drawer.hidden {
  transform: translateX(-100%);
}

.menu-drawer.hidden::before {
  opacity: 0;
}

.menu-drawer-inner {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.menu-button {
  position: relative;
  cursor: pointer;
  transition: 0.15s;
  z-index: 999;

  &:hover {
    padding-left: 15px;
  }
}

.menu-button:hover .menu-arroe-icon {
  opacity: 1;
  left: 0;
}

.menu-arroe-icon {
  transform: rotate(100deg);
  opacity: 0;
  width: 11.5px;
  height: 12px;
  position: absolute;
  top: 5px;
  left: -15px;
  transition: 0.15s;
  pointer-events: none;
}

// Start footer
.footer-inner {
  padding-top: 25px;
  padding-bottom: 60px;
  border-top: 1px solid black;

  @include phone {
    flex-direction: column;
  }
}

.footer-left,
.footer-right {
  width: 100%;
  padding-bottom: 25px;
}

@media (hover: hover) and (pointer: fine) {
  .footer-menu a {
    transition: color 0.5s;
  }

  .footer-menu ul:hover a {
    color: white;
  }

  .footer-menu li a:hover {
    color: black;
  }
}

// Start hero section
.hero-image {
  @include phone {
    height: 270px;

    img {
      // height: 100%;
      // object-fit: cover;
    }
  }
}

// Start rich text section
.heading {
  font-size: 34px;
}

.centerC {
  text-align: center;
}

.padding-top {
  padding-top: 25px;
}

// Image with text section
.image-with-text {
  display: grid;
  grid-template-columns: 1fr 1fr;

  @include phone {
    grid-template-columns: 1fr;
  }
}

.img-text {
  width: 100%;
  vertical-align: middle;
}

.image-with-text.reverse {
  direction: rtl;
}

.image-with-text-description-container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  // padding: 25px;
  padding: 25px 0px;
  direction: ltr;
}

.image-with-text-body {
  width: 75%;
  margin: 0 auto;
  padding-top: 50px;
  text-align: center;
}

// Start logo list
.logo-container {
  display: flex;
  justify-content: space-between;
  padding-top: 50px;
}

.logo-link {
  max-width: 25%;
  display: flex;
  justify-content: center;
  align-self: center;

  img {
    width: 50%;
  }
}

// image with text

.banner {
  display: flex;
  justify-content: center;
}

.img-with-text-button {
  // font-family: Patua One;
  display: flex;
  align-items: center;
  width: 1542px;
  height: 430px;
  border-radius: 30px;
  gap: 58px;
  padding: 85px 103px 85px 92px;
  margin: 60px 25px;
  transition: all 0.3s ease;
  background-color: var(--background-color);
  color: var(--text-color);

  @include phone() {
    display: flex;
    flex-direction: column;
    height: auto;
    border-radius: 20px;
    padding: 20px;
    align-items: flex-start;
  }
}

.img-with-text-button:hover {
  background-color: var(--hover-background-color) !important;
}

.img-with-text-button:hover .text-with-button h2 {
  color: var(--hover-text-color) !important;
}

.img-with-text-button:hover .text-with-button p {
  color: var(--hover-description-color) !important;
}

.img-with-text-button:hover .text-with-button .clickButton {
  margin-left: 0;
}

.img-with-text-button:hover .clickButton a {
  color: var(--hover-button-text-color) !important;
}

.img-with-text-button:hover .clickButton {
  background-color: var(--hover-button-background-color) !important;
}

.image-section {
  width: 45%;
}

.image-section img {
  height: 444px;

  margin-top: -28px;

  @include phone() {
    height: 235px !important;
    width: 295px !important;
    margin-top: -50px;
  }
}

.text-with-button {
  width: 55%;

  @media (max-width: 1076px) {
    width: 100%;
  }
}

.text-with-button h2 {
  font-family: Patua One, serif;
  font-size: 50px;
  line-height: 62px;
  padding-bottom: 12px;
  font-weight: 400;
  text-align: left;
  color: var(--text-color);

  @include phone() {
    font-family: Patua One, serif;
    font-weight: 400;
    font-size: 24px;
    line-height: 34px;
    margin-bottom: 12px;
  }
}

.text-with-button h2 {
  color: var(--text-color) !important;
}

.text-with-button p {
  color: var(--description-color);
}

.text-with-button .text-body {
  font-family: "Assistant", serif;
  font-size: 18px;
  font-weight: 400;
  line-height: 24px;
  text-align: left;
  margin-bottom: 24px;
}

.text-with-button .hide-mobile {
  @include phone {
    display: none !important;
  }
}

@media (max-width: 1076px) {
  .text-with-button h2 {
    font-family: Patua One, serif;
    font-weight: 400;
    font-size: 24px;
    line-height: 34px;
    margin-bottom: 12px;
  }

  .image-section img {
    height: 400px;
    width: 250px;
    margin-top: -70px;
  }

  .promotional-img img{
    width: 250px !important;
  }
}

@media (max-width: 776px) {
  .text-with-button h2 {
    font-family: Patua One, serif;
    font-weight: 400;
    font-size: 24px;
    line-height: 34px;
    margin-bottom: 12px;
  }

  .text-with-button p {
    display: none;
  }
}

.text-with-button .clickButton {
  width: 224px;
  height: 52px;
  border-radius: 98px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24px;
  line-height: 36px;
  margin-left: -60px;
  position: relative;
  transition: all 0.5s ease;
  background-color: var(--button-background-color);
  text-decoration: none;

  @media (max-width: 1076px) {
