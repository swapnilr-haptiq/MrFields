    background-color: var(--mobile-button-bg-color);
  }
}

.text-with-button .clickButton a {
  color: var(--button-text-color);

  @include phone() {
    color: var(--mobile-button-text-color);
  }
}

.text-with-button button {
  position: relative;
  left: 0;
  transition: all 0.5s ease;
}

.custom-button {
  font-family: Assistant, serif;
  border: none;
  cursor: pointer;
  text-decoration: none;
  display: inline-block;
  font-weight: bold;
  background-color: transparent;
  outline: none;
}

.a-container .img-with-text-button .image-section img {
  width: 100%;
}

// promotional banner season

.promotional-season {
  // font-family: Patua One;
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
  width: 1542px;
  height: 430px;
  border-radius: 30px;
  gap: 58px;
  padding: 87px 102px;
  margin: 25px;
  transition: all 0.3s ease;

  @include phone() {
    display: flex;
    flex-direction: column;
    height: auto;
    border-radius: 20px;
    padding: 20px;
  }
}

.promotional-season:hover {
  background-color: var(--hover-background-color) !important;

  .text-with-button .clickButton {
    margin-left: 0;
  }

  .text-with-button h2,
  p {
    color: white;
  }
}

.season-image-section {
  width: 50%;
  display: flex;
  justify-content: center;
}

.season-image-section img {
  display: flex;
  align-items: center;
  height: 542px;
  width: 265px;
  margin-top: -64px;

  @include phone(){
    height: 265px !important;
    width: 129px !important;
    margin-top: -90px;
  }

}

 @media (max-width: 1076px) {
  .season-image-section img{
    height: 400px;
        width: 200px;
        margin-top: -100px;
  }
 }

//  featured Collection

.featured-collection {
  padding: 130px 0;
  background-color: var(--background-color) !important;
  color: var(--text-color);
}

.featured-collection .container {
  max-width: 1470px;
  margin: 0 auto;
  overflow: visible;
}

.section-title {
  font-family: "Patua One", serif;
  font-size: 52px;
